        "codeat3/blade-cryptocurrency-icons": "^1.3",
        "codeat3/blade-devicons": "^1.2",
        "codeat3/blade-elusive-icons": "^1.1",
        "codeat3/blade-emblemicons": "^1.0",
        "codeat3/blade-eos-icons": "^1.9",
        "codeat3/blade-evil-icons": "^1.1",
        "codeat3/blade-file-icons": "^1.5",
        "codeat3/blade-fluentui-system-icons": "^1.10",
        "codeat3/blade-fontaudio": "^1.1",
        "codeat3/blade-fontisto-icons": "^1.0",
        "codeat3/blade-forkawesome": "^1.6",
        "codeat3/blade-google-material-design-icons": "^1.4",
        "codeat3/blade-govicons": "^1.0",
        "codeat3/blade-grommet-icons": "^1.2",
        "codeat3/blade-iconpark": "^1.3",
        "codeat3/blade-ikonate": "^1.1",
        "codeat3/blade-jam-icons": "^1.2",
        "codeat3/blade-line-awesome-icons": "^1.0",
        "codeat3/blade-majestic-icons": "^1.0",
        "codeat3/blade-maki-icons": "^1.1",
        "codeat3/blade-microns": "^1.1",
        "codeat3/blade-mono-icons": "^1.0",
        "codeat3/blade-pepicons": "^1.0",
        "codeat3/blade-phosphor-icons": "^1.2",
        "codeat3/blade-pixelarticons": "^1.1",
        "codeat3/blade-prime-icons": "^1.1",
        "codeat3/blade-radix-icons": "^1.1",
        "codeat3/blade-rpg-awesome-icons": "^1.1",
        "codeat3/blade-simple-line-icons": "^1.0",
        "codeat3/blade-system-uicons": "^1.1",
        "codeat3/blade-teeny-icons": "^1.5",
        "codeat3/blade-typicons": "^1.3",
        "codeat3/blade-uiw-icons": "^1.1",
        "codeat3/blade-unicons": "^1.3",
        "codeat3/blade-vaadin-icons": "^1.1",
        "codeat3/blade-weather-icons": "^1.2",
        "davidhsianturi/blade-bootstrap-icons": "^1.1",
        "faisal50x/blade-ionicons": "^0.0.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hasnayeen/blade-eva-icons": "^1.0",
        "itsmalikjones/blade-iconic": "^1.2",
        "khatabwedaa/blade-css-icons": "^1.0",
        "laravel/framework": "^8.65",
        "laravel/jetstream": "^2.4",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.5",
        "mallardduck/blade-boxicons": "^2.1",
        "mallardduck/blade-lucide-icons": "^1.4",
        "nerdroid23/blade-icomoon": "^1.0",
        "owenvoke/blade-entypo": "^1.5",
        "owenvoke/blade-fontawesome": "^1.10",
        "ryangjchandler/blade-tabler-icons": "^1.1",
        "spatie/browsershot": "^3.52",
        "spatie/laravel-medialibrary": "^9.9",
        "statamic/cms": "^3.2",
        "stijnvanouplines/blade-country-flags": "^1.0",
        "troccoli/blade-health-icons": "^0.2.0",
        "ublabs/blade-coreui-icons": "^0.1.0",
        "ublabs/blade-simple-icons": "^0.17.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-update-cmd": [
            "Statamic\\Console\\Composer\\Scripts::preUpdateCmd"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan statamic:install --ansi",
            "@php artisan statamic:search:update --all --ansi",
            "@php artisan statamic:static:clear --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
